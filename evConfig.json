{
  "reporterOptions": {
    "reportFormat": "json",
    "fileName": "aggregatedReportForRun.json",
    "outputDir": "./evincedReports",
    "tmpDir": "./evincedReports/tmp"
  },
  "uploadToPlatformOptions": {
    "enableUploadToPlatform": true,
    "setUploadToPlatformDefault": false
  },
  "skipValidations": [
    {
      "selector": ".dropdown.line",
      "urlRegex": ".*demo.evinced.com\/",
      "validationTypes": [
        "WRONG_SEMANTIC_ROLE"
      ]
    },
    {
      "selector": ".search-btn",
      "urlRegex": ".*demo.evinced.com\/",
      "validationTypes": [
        "NOT_FOCUSABLE"
      ]
    },
    {
      "selector": "mdn-docs-logo",
      "urlRegex": ".*developer.mozilla.org/en-US/docs/Web/CSS/:nth-child#try_it\/",
      "validationTypes": [
        "LOW_FOCUS_CONTRAST"
      ]
    }
  ]
}